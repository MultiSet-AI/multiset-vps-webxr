{
  "name": "multiset-vps-webxr",
  "version": "1.0.0",
  "description": "Multiset VPS WebXR SDK - Core client and WebXR controller for framework-agnostic usage.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs",
      "default": "./dist/core/index.js"
    },
    "./webxr": {
      "types": "./dist/webxr/index.d.ts",
      "import": "./dist/webxr/index.js",
      "require": "./dist/webxr/index.cjs",
      "default": "./dist/webxr/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "npm run build"
  },
  "keywords": [
    "multiset",
    "webxr",
    "three",
    "ar",
    "localization",
    "vps",
    "visual-positioning",
    "augmented-reality"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multiset/multiset-vps-webxr.git"
  },
  "peerDependencies": {
    "three": ">=0.176.0"
  },
  "devDependencies": {
    "@types/three": "^0.176.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^9.1.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}

